{
  "name": "deliver-ease",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development.localpc next dev -p 3001",
    "agarh": "env-cmd -f .env.agarh.production next start -p 3002",
    "demo": "env-cmd -f .env.demo.production next start -p 3003",
    "ftalal": "env-cmd -f .env.ftalal.production next start -p 3004",
    "alinsaf": "env-cmd -f .env.alinsaf.production next start -p 3005",
    "build-agarh": "env-cmd -f .env.agarh.production next build",
    "build-ftalal": "env-cmd -f .env.ftalal.production next build",
    "build-alinsaf": "env-cmd -f .env.alinsaf.production next build",
    "build-demo": "env-cmd -f .env.demo.production next build",
    "build-local": "env-cmd -f .env.development.localpc next build",
    "deploy-agarh": "pm2 stop 8 && git pull && rm -rf .next && env-cmd -f .env.agarh.production npm run build-agarh && pm2 start 8",
    "deploy-ftatal": "pm2 stop 5 && git pull && rm -rf .next && env-cmd -f .env.ftalal.production npm run build-ftalal && pm2 start 5",
    "deploy-alinsaf": "pm2 stop 6 && git pull && git merge origin/development && rm -rf .next && env-cmd -f .env.alinsaf.production npm run build-alinsaf && pm2 start 6",
    "deploy-demo": "pm2 stop 7 && git pull && rm -rf .next && env-cmd -f .env.demo.production npm run build-demo && pm2 start 7",
    "merge": "git pull && git merge origin/development"
  },
  "dependencies": {
    "@createnextapp/react-barcode": "^1.1.1",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@mui/lab": "^7.0.0-beta.11",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "cogo-toast": "^4.2.3",
    "cuid": "^3.0.0",
    "env-cmd": "^10.1.0",
    "export-from-json": "^1.4.1",
    "firebase": "^11.6.1",
    "formik": "^2.2.1",
    "formik-material-ui": "^4.0.0-alpha.2",
    "formik-material-ui-lab": "1.0.0-alpha.2",
    "framer-motion": "^12.9.2",
    "localforage": "^1.9.0",
    "material-table": "^2.0.6",
    "mobx": "6.13.7",
    "mobx-react-lite": "4.1.0",
    "mobx-state-tree": "7.0.2",
    "next": "15.3.1",
    "next-images": "1.8.5",
    "postcss": "8",
    "postcss-preset-env": "^10.1.6",
    "postcss-safe-parser": "^7.0.1",
    "postcss-scss": "^4.0.9",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-images-uploading": "^3.1.2",
    "react-js-pagination": "^3.0.3",
    "react-swipeable-views": "^0.14.0",
    "react-to-print": "^3.1.0",
    "swr": "^2.3.3",
    "use-async-effect": "^2.2.2",
    "use-async-memo": "^1.2.3",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": "20.x"
  }
}
